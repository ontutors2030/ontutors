@{
    ViewData["Title"] = "Home Page";
}
@if (!User.Identity.IsAuthenticated || !User.IsInRole("Student"))
{
    <div class="text-center">
        <div class="row">
            <div class="col-md-5">
                <br /><br /><br /><br />
                <div style="font-size:80px;">
                    Online private tutor finder
                </div>
                <br /><br /><br />
                @if (!User.Identity.IsAuthenticated)
                {
                    <a class="btn btn-lg defaultBtn" style="width:200px;font-weight:bold;" asp-area="Identity" asp-page="/Account/Register">Register Now!</a>
                }
            </div>

            <div class="col-md-7">
                <br /><br />
                @*<img src="~/imgs/userPC.jfif" class="img-responsive" style="height:600px;width:500px;" />*@
                <img src="~/imgs/userPC.jfif" class="img-responsive img-thumbnail" style="height:600px;width:500px;" />
            </div>
        </div>
    </div>
}
else if (User.Identity.IsAuthenticated && User.IsInRole("Student"))
{
   <div class="row">
            <div id="divCourses">

            </div>
        </div>
        @section Scripts {
            <script>
                $(document).ready(function () {
                    $("#divCourses").load("../Home/CourseSearchResult", function () {
                        setRating();
                    });
                });
            </script>
        }

